{
    "configurations": [
        {
            "name": "smallweb serve",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "serve",
                "${workspaceFolder}/examples"
            ]
        },
        {
            "name": "smallweb proxy",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "proxy"
            ]
        },
        {
            "name": "smallweb tunnel",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "tunnel",
                "${workspaceFolder}/examples"
            ],
            "console": "integratedTerminal"
        }
    ]
}
