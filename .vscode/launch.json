{
    "configurations": [
        {
            "name": "smallweb up",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "up",
            ]
        },
        {
            "name": "smallweb run",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "run",
                "${input:app}"
            ]
        }
    ],
    "inputs": [
        {
            "type": "promptString",
            "id": "app",
            "description": "Enter the app name",
        }
    ]
}
